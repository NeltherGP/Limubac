<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="limubac\administratorBundle\Entity\Usuario" table="usuario">
    <indexes>
      <index name="usuario_ibfk_1" columns="id_rol"/>
    </indexes>
    <id name="idUsuario" type="integer" column="id_usuario">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nombre" type="string" column="nombre" length="255" nullable="false"/>
    <field name="apellido" type="string" column="apellido" length="255" nullable="false"/>
    <field name="telefono" type="integer" column="telefono" nullable="false"/>
    <field name="correo" type="string" column="correo" length="255" nullable="false"/>
    <field name="credito" type="integer" column="credito" nullable="false"/>
    <many-to-one field="idRol" target-entity="Rol">
      <join-columns>
        <join-column name="id_rol" referenced-column-name="id_rol"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
