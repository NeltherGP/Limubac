<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="limubac\administratorBundle\Entity\Partido" table="partido">
    <indexes>
      <index name="id_torneo" columns="id_torneo,id_arbitran,id_sede"/>
      <index name="id_arbitran" columns="id_arbitran"/>
      <index name="id_sede" columns="id_sede"/>
      <index name="IDX_4E79750B5ADCD613" columns="id_torneo"/>
    </indexes>
    <id name="idPartido" type="integer" column="id_partido">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="hInicio" type="time" column="h_inicio" nullable="false"/>
    <field name="hTermino" type="time" column="h_termino" nullable="false"/>
    <field name="commited" type="boolean" column="commited" nullable="true"/>
    <field name="primero" type="integer" column="primero" nullable="true"/>
    <field name="segundo" type="integer" column="segundo" nullable="true"/>
    <field name="tercero" type="integer" column="tercero" nullable="true"/>
    <field name="cuarto" type="integer" column="cuarto" nullable="true"/>
    <many-to-one field="idSede" target-entity="Sede">
      <join-columns>
        <join-column name="id_sede" referenced-column-name="id_sede"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idArbitran" target-entity="Arbitran">
      <join-columns>
        <join-column name="id_arbitran" referenced-column-name="id_arbitran"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idTorneo" target-entity="Torneo">
      <join-columns>
        <join-column name="id_torneo" referenced-column-name="id_torneo"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
